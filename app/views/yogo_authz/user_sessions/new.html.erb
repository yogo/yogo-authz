<h1>YOGO AUTHZ</h1>

<% form_for @user_session, :url => user_session_path do |f| %>
  <%= f.error_messages %>
  <%= f.label(:ldap_login, 'Login') %><br />
  <%= f.text_field(:ldap_login) %><br />
  <br />
  <%= f.label(:ldap_password, 'Password') %><br />
  <%= f.password_field(:ldap_password) %><br />
  <br />
  <%= f.check_box :remember_me %><%= f.label :remember_me %><br />
  <br />
  <%= f.submit "Login" %>
<% end %>